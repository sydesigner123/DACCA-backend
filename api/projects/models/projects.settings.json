{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "support": {
      "type": "text",
      "maxLength": 350
    },
    "cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "tags": {
      "collection": "tags",
      "via": "projects",
      "dominant": true
    },
    "sector": {
      "via": "projects",
      "model": "sector"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "introduction": {
      "type": "dynamiczone",
      "components": [
        "editing-components.image",
        "editing-components.image-row",
        "editing-components.text",
        "editing-components.image-gallery"
      ]
    },
    "conceptAndDesign": {
      "type": "dynamiczone",
      "components": [
        "editing-components.image-row",
        "editing-components.image",
        "editing-components.text",
        "editing-components.image-gallery"
      ]
    },
    "technical": {
      "type": "dynamiczone",
      "components": [
        "editing-components.image-row",
        "editing-components.image",
        "editing-components.text",
        "editing-components.image-gallery"
      ]
    },
    "projectStatus": {
      "model": "project-status"
    },
    "subsector": {
      "model": "subsector",
      "via": "projectsList"
    }
  }
}
